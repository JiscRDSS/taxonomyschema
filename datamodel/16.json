{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Relation Type",
  "description": "Vocabulary used to relate digital objects and records with one another.",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for the vocabulary",
      "default": 16,
      "type": "integer"
    },
    "vocabulary": {
      "description": "Relational vocabulary to link digital objects and express the relationship.",
      "type": "string",
      "enum": ["cites", "isCitedBy", "isSupplementTo", "isSupplementedBy", "continues", "isContinuedBy", "hasMetadata", "isMetadataFor", "isNewVersionOf", "isPreviousVersionOf", "isPartOf", "hasPart", "isReferencedBy", "references", "isDocumentedBy", "documents", "isCompiledBy", "compiles", "isVariantFormOf", "isOriginalFormOf", "isIdenticalTo", "isReviewedBy", "reviews", "isDerivedFrom", "isSourceOf", "isCommentOn", "hasComment", "isReplyTo", "hasReply", "basedOnData", "hasRelatedMaterial", "isBasedOn", "isBasisFor", "requires", "isRequiredBy", "hasParent", "isParentOf"]
    }
  },
  "required": ["id", "vocabulary"]
}
