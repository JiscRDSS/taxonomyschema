{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Access Type",
  "description": "Access type assigned to the data",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for the vocabulary",
      "default": 1,
      "type": "integer"
    },
    "vocabulary": {
      "description": "The access type",
      "type": "string",
      "enum": ["open", "restricted", "controlled", "closed"]
    }
  },
  "required": ["id", "vocabulary"]
}
