{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Dataset Resource Type",
  "description": "Type of data being produced",
   "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for the vocabulary",
      "default": 4,
      "type": "integer"
    },
    "vocabulary": {
      "description": "Single word classication of the data that has been produced.",
      "type": "string",
      "enum": ["dataset", "image", "movingImage", "model", "software", "audio", "other", "interactiveResource", "event", "service", "text", "workflow"]
    }
  },
  "required": ["id", "vocabulary"]
}
