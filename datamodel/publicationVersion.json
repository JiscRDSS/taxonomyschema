{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Publication Version",
  "description": "NISO RP-8-201x Journal Article Versions (JAV)",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for the vocabulary",
      "default": 15,
      "type": "integer"
    },
    "vocabulary": {
      "description": "The journal article version (JAV) as set out by the NISO RP-8-201x international standard. ",
      "type": "string",
      "enum": ["AO", "SMUR", "AM", "P", "VoR", "CVoR", "EVoR", "NA"]
    }
  },
  "required": ["id", "vocabulary"]
}
