{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "APC",
  "description": "Article Processing Charge status",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for the vocabulary",
      "default": 1,
      "type": "integer"
    },
    "vocabulary": {
      "description": "The APC",
      "type": "string",
      "enum": ["paid", "partiallyWaived", "fullyWaived", "notCharged", "notRequired", "unknown"]
    }
  },
  "required": ["id", "vocabulary"]
}
